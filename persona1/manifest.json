{
    "manifest_version": 2,
    "name": "Persona Request Modifier",
    "version": "1.0",
    "permissions": [
    "tabs",
    "activeTab",
    "windows",
    "management",
    "notifications",
    "management",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback"
  ],
    "background": {
      "service_worker": "background.js"
    }
  }
  