{
    "manifest_version": 3,
    "name": "Bypass Verification Extension",
    "version": "1.0",
    "description": "Bypass verification button on certain pages.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
      
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
    
  }
  